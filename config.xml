<?xml version='1.0' encoding='utf-8'?>
<widget id="com.waypoolsas.waypooldriver2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Waypool Cars</name>
    <description>Waypool for students with car</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" src="resources/android/icon/drawable-mdpi/ic_launcher.png" />
        <icon density="xhdpi" src="resources/android/icon/drawable-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="resources/android/icon/drawable-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="resources/android/icon/drawable-xxxhdpi/ic_launcher.png" />
        <splash density="land-ldpi" src="resources/android/splash/splash.png" />
    </platform>
    <engine name="ios" spec="4.5.5" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^1.2.1" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.1.3" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value="To locate you" />
        <variable name="cordova-plugin-camera" value="[object Object]" />
    </plugin>
    <plugin name="platforms">
        <variable name="undefined" value="undefined" />
        <variable name="undefined" value="undefined" />
    </plugin>
    <plugin name="mx.ferreyra.callnumber" spec="0.0.2" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-email-composer" spec="^0.9.2">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="call-number" spec="^1.0.1" />
    <plugin name="cordova-plugin-badge" spec="^0.8.8" />
    <plugin name="cordova-sqlite-storage" spec="^3.2.0" />
    <engine name="android" spec="~7.1.0" />
</widget>
